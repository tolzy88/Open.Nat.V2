<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>netstandard2.0</TargetFrameworks>
        <Platforms>AnyCPU;x64;x86</Platforms>
        <PackageId>Open.Nat.V2</PackageId>
        <Authors>tolzy88, Alan McGovern, Ben Motmans, Lucas Ontivero</Authors>
        <Product>Open.Nat.V2</Product>
        <PackageTags>Networking</PackageTags>
        <Description>.NET Library for automatic network address translation</Description>
        <NeutralLanguage>en</NeutralLanguage>
        <Copyright>Copyright Alan McGovern, Ben Motmans, Lucas Ontivero Â©  2006-2014</Copyright>
        <OutputType>Library</OutputType>
    </PropertyGroup>

	<PropertyGroup>
		<IsAotCompatible>true</IsAotCompatible>
	</PropertyGroup>

    <ItemGroup>
	    <None Include="..\LICENSE" Pack="true" PackagePath="" Visible="false" />
	    <None Include="..\README.md" Pack="true" PackagePath="" />
    </ItemGroup>
	
    <PropertyGroup>
        <PackageProjectUrl>https://github.com/tolzy88/Open.Nat.V2</PackageProjectUrl>
        <RepositoryUrl>https://github.com/tolzy88/Open.Nat.V2.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
    </PropertyGroup>
	
    <ItemGroup>
	    <InternalsVisibleTo Include="Open.Nat.UnitTests" />
    </ItemGroup>
	
    <ItemGroup>
        <Compile Remove="Upnp\Messages\Responses\AddPortMappingResponseMessage.cs" />
        <Compile Remove="Upnp\Messages\Responses\DeletePortMappingResponseMessage.cs" />
    </ItemGroup>
	
</Project>